<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/black"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/headertitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:singleLine="true"
                android:text="Hello"
                android:textColor="@android:color/white"
                android:textSize="16sp" />

            <Button
                android:id="@+id/logout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="5dp"
                android:background="@drawable/close_button"
                android:textColor="@android:color/black"
                android:textSize="14sp" />
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:text="Showing 1 to 10 of 100 users"
            android:textColor="@color/black"
            android:textSize="16sp" />

        <LinearLayout
            android:id="@+id/moreusers_layout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="30dp"
            android:orientation="horizontal" >

            <com.goconnect.events.lib.CircularImageView
                android:id="@+id/moredata_user_1"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:onClick="showOtherUserBusinessCard"
                android:scaleType="fitCenter"
                android:src="@drawable/defaultuser" />

            <com.goconnect.events.lib.CircularImageView
                android:id="@+id/moredata_user_2"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:onClick="showOtherUserBusinessCard"
                android:scaleType="fitCenter"
                android:src="@drawable/defaultuser" />

            <com.goconnect.events.lib.CircularImageView
                android:id="@+id/moredata_user_3"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:onClick="showOtherUserBusinessCard"
                android:scaleType="fitCenter"
                android:src="@drawable/defaultuser" />

            <com.goconnect.events.lib.CircularImageView
                android:id="@+id/moredata_user_4"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:onClick="showOtherUserBusinessCard"
                android:scaleType="fitCenter"
                android:src="@drawable/defaultuser" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/moreusers_layout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal" >

            <com.goconnect.events.lib.CircularImageView
                android:id="@+id/moredata_user_5"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:onClick="showOtherUserBusinessCard"
                android:scaleType="fitCenter"
                android:src="@drawable/defaultuser" />

            <com.goconnect.events.lib.CircularImageView
                android:id="@+id/moredata_user_6"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:onClick="showOtherUserBusinessCard"
                android:scaleType="fitCenter"
                android:src="@drawable/defaultuser" />

            <com.goconnect.events.lib.CircularImageView
                android:id="@+id/moredata_user_7"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:onClick="showOtherUserBusinessCard"
                android:scaleType="fitCenter"
                android:src="@drawable/defaultuser" />

            <com.goconnect.events.lib.CircularImageView
                android:id="@+id/moredata_user_8"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:onClick="showOtherUserBusinessCard"
                android:scaleType="fitCenter"
                android:src="@drawable/defaultuser" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/moreusers_layout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal" >

