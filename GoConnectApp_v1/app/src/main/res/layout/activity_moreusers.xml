                android:id="@+id/moredata_user_9"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:onClick="showOtherUserBusinessCard"
                android:scaleType="fitCenter"
                android:src="@drawable/defaultuser" />

            <com.goconnect.events.lib.CircularImageView
                android:id="@+id/moredata_user_10"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:onClick="showOtherUserBusinessCard"
                android:scaleType="fitCenter"
                android:src="@drawable/defaultuser" />

            <com.goconnect.events.lib.CircularImageView
                android:id="@+id/moredata_user_11"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:onClick="showOtherUserBusinessCard"
                android:scaleType="fitCenter"
                android:src="@drawable/defaultuser" />

            <com.goconnect.events.lib.CircularImageView
                android:id="@+id/moredata_user_12"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:onClick="showOtherUserBusinessCard"
                android:scaleType="fitCenter"
                android:src="@drawable/defaultuser" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/moreusers_layout4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal" >

            <com.goconnect.events.lib.CircularImageView
                android:id="@+id/moredata_user_13"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:onClick="showOtherUserBusinessCard"
                android:scaleType="fitCenter"
                android:src="@drawable/defaultuser" />

            <com.goconnect.events.lib.CircularImageView
                android:id="@+id/moredata_user_14"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:onClick="showOtherUserBusinessCard"
                android:scaleType="fitCenter"
                android:src="@drawable/defaultuser" />

            <com.goconnect.events.lib.CircularImageView
                android:id="@+id/moredata_user_15"
                android:layout_width="50dp"
